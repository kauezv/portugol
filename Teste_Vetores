algoritmo "TesteVetores"
var
  v: vetor[1..6] de inteiro
  c: inteiro
inicio
  Para c <- 1 ate 6 faca
       Escreva("Digite o ", c, "o. valor: ")
       Leia(v[c])
  FimPara
  
  Escreval(" ")
  Escreval("Os valores definidos foram: ")
  Para c <- 1 ate 6 faca
       Escreval("{",v[c], " }")
  FimPara
fimalgoritmo
