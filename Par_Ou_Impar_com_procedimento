algoritmo "Par_Ou_Impar_com_procedimento"
var
  N: inteiro
  
Procedimento ParOuImpar(V: inteiro)
inicio
  Se (V%2 = 0) entao
     Escreval ("O número",V, " é Par")
  senao
     Escreval ("O número",V, " é Impar")
  FimSe
FimProcedimento

inicio
  Escreva("Digite um número: ")
  Leia (N)
  ParOuImpar(N)
fimalgoritmo
